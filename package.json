        "app-module-path": "^2.2.0",
        "archiver": "^5.3.0",
        "better-sqlite3": "^11.3.0",
        "bootstrap": "^5.3.0",
        "chokidar": "^4.0.0",
        "classnames": "^2.2.6",
        "command-exists": "^1.2.9",
        "css": "^3.0.0",
        "csv-parse": "^5.4.0",
        "csv-stringify": "^6.4.0",
        "d3-format": "^3.1.0",
        "decompress": "^4.2.1",
        "deep-equal": "^2.0.5",
        "electron-context-menu": "^3.6.1",
        "fast-deep-equal": "^3.1.3",
        "file-type": "^8.1.0",
        "flexlayout-react": "^0.7.15",
        "fourier-transform": "^1.1.2",
        "fs-extra": "^6.0.1",
        "immutability-helper": "^3.1.1",
        "jquery": "^3.5.1",
        "jsmodbus": "^4.0.6",
        "json-stable-stringify": "^1.0.1",
        "jspanel4": "^4.16.1",
        "koffi": "^2.6.2",
        "lodash": "^4.17.21",
        "luxon": "^3.4.4",
        "lv_font_conv": "github:lvgl/lv_font_conv",
        "marked": "^5.1.1",
        "mobx": "6.5.0",
        "mobx-react": "^7.3.0",
        "mobx-utils": "^6.0.4",
        "moment": "^2.29.2",
        "moment-duration-format": "^2.3.2",
        "mousetrap": "^1.6.5",
        "mqtt": "^4.3.7",
        "opentype.js": "^1.3.4",
        "peggy": "^1.2.0",
        "plotly.js-dist-min": "^2.32.0",
        "python-shell": "^3.0.1",
        "quill": "^1.3.7",
